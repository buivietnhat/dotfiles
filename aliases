alias mdkir=mkdir

alias dc=cd

alias sl=ls

alias gs="git s"

alias gl="git l"

alias nd="nvim -d"

# will prompt will overwrite
alias mv="mv -i"

# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias vi=nvim
alias vim=nvim

alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"

# Print each PATH entry on a separate line
alias path='echo -e ${PATH//:/\\n}'
